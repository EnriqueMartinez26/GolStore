{
    "version": 2,
    "builds": [
      {
        "src": "backend/server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "backend/server.js"
      }
    ],
    "env": {
      "MONGODB_URI": "mongodb://localhost:27017/golstore",
      "PORT": "5000"
    }
  }
  